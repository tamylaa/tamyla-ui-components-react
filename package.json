{
  "name": "@tamyla/ui-components-react",
  "version": "5.0.0",
  "description": "React-based UI component library with Factory Bridge pattern - integrates seamlessly with @tamyla/ui-components. Enhanced AI agent discoverability with structured component registry, comprehensive Storybook (8 components), and detailed guides.",
  "type": "module",
  "sideEffects": [
    "**/*.css",
    "**/store/**",
    "**/index.ts",
    "**/utils/async-safety.ts",
    "**/utils/dom-safety.ts",
    "**/utils/factory-health-monitor.ts"
  ],
  "main": "dist/index.esm.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "types": "./dist/index.d.ts"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "src/core/design-tokens.css",
    "README.md",
    "API_REFERENCE.md",
    "MIGRATION_GUIDE.md",
    "TROUBLESHOOTING.md",
    "COMPONENT_DISCOVERY_GUIDE.md",
    "DESIGN_TOKENS_GUIDE.md",
    "STORYBOOK_GUIDE.md",
    "TYPESCRIPT_NAVIGATION_GUIDE.md",
    "DEVELOPER_EXPERIENCE_ENHANCEMENT_REPORT.md",
    "component-registry.json",
    "scripts/dev/dev-utils.js",
    "scripts",
    "docs",
    "examples"
  ],
  "scripts": {
        "build": "npm run clean && rollup -c rollup.config.js",
    "postbuild": "npm run check:bundle",
    "build:types": "tsc --emitDeclarationOnly",
    "build:bundle": "rollup -c",
    "clean": "rimraf dist",
    "dev": "rollup -c -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "lint": "eslint --config ./eslint.config.js src",
    "lint:fix": "eslint --config ./eslint.config.js src --fix",
    "type-check": "tsc --noEmit",
    "check:cjs": "node scripts/test/check-cjs.js",
    "check:bundle": "node scripts/check-bundle.js",
    "debug:bundle": "node scripts/analyze-bundle-imports.js",
    "verify:fix": "node scripts/verify-bundle-fix.js",
    "prepublishOnly": "npm run registry:combine && npm run certify-exports && npm run type-check && npm run check:cjs && npm run check:bundle && npm run build",
    "publish:beta": "npm publish --tag beta",
    "publish:latest": "npm publish --tag latest",
    "analyze": "npm run build && npx bundle-analyzer dist/index.esm.js",
    "validate": "node scripts/esm/basic-validate.js",
    "status": "node scripts/esm/repo-status.js",
    "certify": "node scripts/esm/working-certify.js",
    "complete": "node scripts/esm/complete-automation.js",
    "certify-exports": "node scripts/build/export-certification.js",
    "certify-comprehensive": "node scripts/build/comprehensive-certify.js",
    "certify-zero-issues": "node scripts/build/comprehensive-certify.js",
    "quick-certify": "node scripts/esm/working-certify.js",
    "deploy": "node scripts/esm/deploy-to-github.js",
    "preprod:patch": "node scripts/preprod-workflow.js patch",
    "preprod:minor": "node scripts/preprod-workflow.js minor",
    "preprod:major": "node scripts/preprod-workflow.js major",
    "preprod:custom": "node scripts/preprod-workflow.js custom",
    "preprod:validate": "node scripts/preprod-workflow.js custom --dry-run --verbose",
    "workflow:report": "node scripts/generate-workflow-report.js",
    "workflow:dashboard": "node scripts/workflow-dashboard.js",
    "fix-any-types": "node scripts/fix-any-types.js",
    "docs:api": "echo 'API Reference: API_REFERENCE.md'",
    "docs:migration": "echo 'Migration Guide: MIGRATION_GUIDE.md'",
    "docs:troubleshooting": "echo 'Troubleshooting: TROUBLESHOOTING.md'",
    "docs:registry": "echo 'Component Registry: component-registry.json'",
    "docs:examples": "echo 'Examples available in examples/ directory'",
    "docs:storybook": "npm run storybook",
    "registry:split": "node scripts/build/split-registry.js split",
    "registry:combine": "node scripts/build/split-registry.js combine",
    "registry:status": "node -e \"const fs=require('fs'); const r=JSON.parse(fs.readFileSync('component-registry.json','utf8')); const cats={}; Object.values(r.components).forEach(c=>cats[c.category]=(cats[c.category]||0)+1); console.log('Registry Status:'); console.log('- Components:', Object.keys(r.components).length); console.log('- Categories:', Object.keys(cats).map(c=>c+':'+cats[c]).join(', ')); console.log('- Last updated:', r.metadata?.lastUpdated || 'Unknown');\"",
    "dev:discover": "node scripts/dev/dev-utils.js discover",
    "dev:components": "node scripts/dev/dev-utils.js discover",
    "dev:tokens": "node scripts/dev/dev-utils.js tokens",
    "dev:patterns": "node scripts/dev/dev-utils.js patterns",
    "dev:migrate": "node scripts/dev/dev-utils.js migrate",
    "dev:troubleshoot": "node scripts/dev/dev-utils.js troubleshoot",
    "dev:help": "node scripts/dev/dev-utils.js help"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.0.1",
    "@types/dompurify": "^3.0.5",
    "dompurify": "^3.2.6",
    "framer-motion": "^10.16.16",
    "react-redux": "^9.0.4",
    "redux-persist": "^6.0.0",
    "styled-components": ">=6.0.0"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.27.1",
    "@eslint/js": "^9.9.1",
    "@reduxjs/toolkit": "^2.0.1",
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@storybook/addon-docs": "^7.6.20",
    "@storybook/addon-links": "^7.6.20",
    "@storybook/react": "^7.6.20",
    "@storybook/react-webpack5": "^7.6.20",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@typescript-eslint/eslint-plugin": "^8.6.0",
    "@typescript-eslint/parser": "^8.6.0",
    "babel-jest": "^29.0.0",
    "chalk": "^5.3.0",
    "eslint": "^9.34.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^6.0.0-rc.1",
    "fs-extra": "^11.2.0",
    "globals": "^16.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rimraf": "^5.0.0",
    "rollup": "^4.9.2",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-esbuild": "^6.2.1",
    "rollup-plugin-filesize": "^10.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "semantic-release": "^22.0.12",
    "storybook": "^7.6.20",
    "styled-components": "^6.1.19",
    "ts-jest": "^29.4.1",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "@tamyla/ui-components": ">=1.0.0",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "styled-components": ">=6.0.0"
  },
  "keywords": [
    "react",
    "components",
    "ui",
    "design-system",
    "typescript",
    "styled-components",
    "redux",
    "atomic-design",
    "factory-bridge",
    "ui-components-react"
  ],
  "author": "Tamyla Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tamylaa/tamyla-ui-components-react.git"
  },
  "bugs": {
    "url": "https://github.com/tamylaa/tamyla-ui-components-react/issues"
  },
  "homepage": "https://ui-react.tamyla.com",
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/npm",
        {
          "npmPublish": true,
          "tarballDir": "dist"
        }
      ],
      "@semantic-release/github"
    ]
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "directories": {
    "example": "examples"
  }
}
